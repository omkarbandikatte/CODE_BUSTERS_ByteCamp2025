{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_VERCEL_URL": "${VERCEL_URL}"
  },
  "buildCommand": "DISABLE_ESLINT_PLUGIN=true next build --no-lint",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "nextjs"
} 